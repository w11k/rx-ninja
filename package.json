  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "dependencies": {
    "@phenomnomnominal/tsquery": "^4.0.0"
  },
  "devDependencies": {
    "@swirly/parser": "^0.15.2",
    "@swirly/renderer-node": "^0.15.2",
    "@swirly/types": "^0.15.2",
    "@types/chai": "^4.2.7",
    "@types/lodash": "^4.14.166",
    "@types/mocha": "^5.2.7",
    "@types/sinon": "^7.5.1",
    "@types/sinon-chai": "^3.2.4",
    "@types/svgo": "^1.3.3",
    "chai": "^4.2.0",
    "color": "^3.1.2",
    "del-cli": "^3.0.0",
    "gh-pages": "^2.1.1",
    "gm": "^1.23.1",
    "lodash": "^4.17.20",
    "mocha": "^6.2.2",
    "rxjs": "^6.5.4",
    "rxjs-marbles": "^5.0.4",
    "sinon": "^8.0.2",
    "sinon-chai": "^3.5.0",
    "svgo": "^1.3.2",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.19.2",
    "typescript": "^3.9.7",
    "version-bump-prompt": "^5.0.7"
  },
  "peerDependencies": {
    "rxjs": ">=6.0.0"
  },
  "scripts": {
    "clean": "del-cli dist",
    "build": "npm run clean ; tsc -p tsconfig.json && tsc -p tsconfig.dist.esm5.json && tsc -p tsconfig.dist.esm2015.json",
    "prepublishOnly": "npm test && npm run build && npm run doc",
    "postpublish-git-tag": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "postpublish-doc": "npm run doc:deploy && git push",
    "postpublish": "npm run postpublish-git-tag ; npm run postpublish-doc",
    "test": "TS_NODE_CACHE=false && mocha --opts spec/mocha.opts src/**/*test.ts",
    "test_tslint_prepare": "npm run clean && tsc",
    "test_tslint_subscribe_takeuntil": "tslint -p tsconfig.json -t verbose -c src/tslint_rules/examples/subscribe-takeuntil/rule.json src/tslint_rules/examples/subscribe-takeuntil/*.ts",
    "test_tslint_subscribe_takeuntil_config": "tslint -p tsconfig.json -t verbose -c src/tslint_rules/examples/subscribe-takeuntil-config/rule.json src/tslint_rules/examples/subscribe-takeuntil-config/*.ts",
    "test_tslint_subscribe_in_subscribe": "tslint -p tsconfig.json -t verbose -c src/tslint_rules/examples/subscribe-in-subscribe/rule.json src/tslint_rules/examples/subscribe-in-subscribe/*.ts",
    "test_tslint": "npm run test_tslint_prepare && npm run test_tslint_subscribe_takeuntil ;npm run test_tslint_subscribe_takeuntil_config ; npm run test_tslint_subscribe_in_subscribe",
    "doc": "npm run doc:generate && touch doc/.nojekyll && git add doc/** && git commit -m 'generated documentation'",
    "doc:generate": "npm run build:marbles && typedoc --options ./typedoc.js src",
    "doc:deploy": "gh-pages -t --dotfiles -d doc",
    "build:marbles": "mkdirp ./tmp/swirly/svgs && ts-node -P ./tools/marbles/tsconfig.marbles.json ./tools/marbles/scripts/index.ts",
    "bump": "bump --commit \"bump version to v\" --no-verify package.json package-lock.json"
  }
}
